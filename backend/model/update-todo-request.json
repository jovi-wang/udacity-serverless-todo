{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "todo",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minProperties": 1,
      "pattern": "^[a-zA-Z0-9]+$"
    },
    "dueDate": {
      "type": "string",
      "format": "date"
    },
    "done": {
      "type": "boolean"
    }
  },
  "required": ["name", "dueDate", "done"],
  "additionalProperties": true
}
